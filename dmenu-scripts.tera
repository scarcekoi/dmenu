---
whiskers:
  version: "^2.5.1"
  matrix:
    - flavor
    - accent
  filename: "scripts/dmenu-{{ flavor.identifier }}-{{ accent }}.sh"
---
{%- set palette = flavor.colors -%}
dmenu_run -nf '#{{ palette.text.hex }}' -nb '#{{ palette.base.hex }}'\
	-sf '#{{ palette.base.hex }}' -sb '#{{ palette[accent].hex }}'\
	-of '#{{ palette.text.hex }}' -ob '#{{ palette[accent].hex }}'
